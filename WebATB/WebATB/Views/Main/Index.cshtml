@using WebATB.Models.Users
@model List<UserItemModel>
@{
    Layout="MainLayout"; // Не використовуємо Layout
}
<h1 class="text-center">Список користувачів</h1>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">ПІБ</th>
            <th scope="col">Телефон</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@user.Name</td>
                <td>@user.Phone</td>
            </tr>
        }
    </tbody>
</table>


